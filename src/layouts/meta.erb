<!-- Contains all meta data contained in the head of the HTML doc -->

<!-- Set our character set to the standard unicode -->
<meta charset="utf-8">

<!-- Set the viewport used by many mobile/desktop browsers -->
<!-- Start width to fill screen, scale is normal, and prevent user zooming -->
<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">

<!-- Friendly indicator that we used nanoc -->
<meta name="generator" content="nanoc <%= Nanoc::VERSION %>">

